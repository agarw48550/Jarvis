"""
  pip install bytez
"""

from bytez import Bytez

import os
key = os.environ.get("BYTEZ_API_KEY")
if not key:
    raise ValueError("BYTEZ_API_KEY environment variable is required")
sdk = Bytez(api_key=key)

# choose zztheaven/Llama-3.2-1B-Instruct-skyt1-GRPO
model = sdk.model("google/gemini-3-pro-preview")

# send input to model

response = model.run([
  {
    "role": "user",
    "content": "Hello"
  }
])

print({ "error": response.error, "output": response.output })